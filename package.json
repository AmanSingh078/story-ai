{
  "name": "aurameter",
  "version": "1.0.0",
  "description": "Indian Gen-Z vibe-based AI aura meter",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
